{
  "name": "nextjs-pdf-cv-generator",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:styles": "npx tailwindcss -o public/build.css --minify ",
    "start": "next start",
    "lint": "next lint",
    "pdf:open": "npm run build:styles && open http://localhost:3000/api/cv",
    "pdf:download": "npm run build:styles && curl -o public/cv.pdf http://localhost:3000/api/pdf"
  },
  "dependencies": {
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "17.0.10",
    "@types/react": "17.0.38",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.2",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "puppeteer": "^13.1.1",
    "tailwindcss": "^3.0.15",
    "typescript": "4.5.5"
  }
}
